<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Links</title>
</head>
<body>
    
    <h2>Links</h2>
    <% links.forEach(link => {%>
        <div id="<%= link.id %>">
            <h2><%= link.title %></h2>
            <div><%= link.description %></div>
            <a href="<%= link.url %>"><%= link.url %></a>
        </div>
    <% });  %>

</body>
</html>